{
  "name": "litestar-pydotorg",
  "version": "0.1.0",
  "description": "Python.org rebuilt with Litestar - Frontend assets",
  "private": true,
  "scripts": {
    "build": "tailwindcss -i ./static/css/input.css -o ./static/css/tailwind.css --minify",
    "watch": "tailwindcss -i ./static/css/input.css -o ./static/css/tailwind.css --watch",
    "dev": "npm run watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "daisyui": "^4.12.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  }
}
